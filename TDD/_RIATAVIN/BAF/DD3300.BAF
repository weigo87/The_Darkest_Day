    TimeOfDay(DUSK)
    TimeOfDay(NIGHT)
  Global("tz_BlackBladesEncounter","DD3300",0)
  !Exists("DDmerc02")
  !Exists("DDBLBL01")
THEN
  RESPONSE #100
    CreateCreature("DDmerc02",[1871.3512],4)
