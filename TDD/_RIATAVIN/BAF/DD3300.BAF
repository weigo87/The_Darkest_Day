END

IF
  OR(2)
    TimeOfDay(DAY)
    TimeOfDay(MORNING)
  Exists("DDMERC02")
  Exists("DDBLBL01")
  Global("tz_BlackBladesEncounter","DD3300",0)
THEN
  RESPONSE #100
    ActionOverride("DDmerc02",DestroySelf())
    ActionOverride("DDBLBL01",DestroySelf())
END

